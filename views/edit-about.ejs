<div class="gallery__container hidden">
  <div class="gallery">
    <div class="close-btn__container">
      <p><i class="fa-solid fa-xmark"></i></p>
    </div>

    <div class="gallery__imgs">
      <div class="panel__spinner-container">
        <i class="fa-solid fa-spinner panel__spinner"></i>
      </div>
    </div>
    <div class="gallery-btns__container hidden">
      <a href="#" class="gallery__prev"
        ><i class="fa-solid fa-arrow-left gallery__page"></i
      ></a>
      <span class="gallery__cur-page">1</span><span>/</span
      ><span class="gallery__max-page">2</span>
      <a href="#" class="gallery__next"
        ><i class="fa-solid fa-arrow-right gallery__page"></i
      ></a>
    </div>
  </div>
</div>

<section class="header__banner-container">
  <div class="header__banner"></div>

  <div class="header__panel" style="max-width: 1140px">
    <div class="panel">
      <p class="articles__latest-posts-heading heading">
        <span class="heading__bar"></span>
        <span class="heading-text">Panel admina</span>
        <span class="heading__bar"></span>
      </p>
      <div class="post__form-container">
        <div class="panel__spinner-overlay">
          <div class="panel__spinner-container">
            <i class="fa-solid fa-spinner panel__spinner"></i>
          </div>
        </div>
        <% if(infoSubmitObj !='') {%>
        <div class="message-container" style="background-color: green">
          Submit succes!
          <!--  -->
          <%= infoSubmitObj %>
        </div>
        <% } %> <% if(infoErrorsObj !='') {%>
        <div class="message-container" style="background-color: red">
          Error!
          <!--  -->
          <%= infoErrorsObj[0].message %> <%= infoErrorsObj%>
        </div>
        <% } %>

        <form
          action="/about/put/6277aa34772ff87e9fe3119c?_method=PUT"
          enctype="multipart/form-data"
          method="POST"
          class="post__form"
        >
          <div class="input-container">
            <label for="title">Tytuł artykułu</label> <br />
            <input
              class="form__input"
              type="text"
              name="title"
              id="name"
              value="<%=about.title%>"
            />
          </div>

          <div class="post__section-container">
            <div class="section__content">
              <div class="input-container">
                <label for="postContent">Treść sekcji</label> <br />
                <small>Wprowadź tekst sekcji</small> <br />

                <textarea
                  class="form__input"
                  name="postContent"
                  id="postContent"
                  cols="30"
                  rows="8"
                >
<%= about.postContent%></textarea
                >
              </div>
            </div>
          </div>

          <div class="btns__container">
            <a href="" class="gallery__btn"
              ><i class="fa-solid fa-camera"></i
            ></a>
            <div class="submit-btn__container">
              <button type="submit" class="btn btn__submit" id="btn__submit">
                Zapisz zmiany
              </button>
            </div>
            <a href="/admin-panel" class="menu__link btn">Wstecz</a>
          </div>
        </form>
      </div>
    </div>
  </div>
</section>
